<AnchorPane prefHeight="594.0" prefWidth="792.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SeanceCodeAvenirController">
   <children>
      <VBox layoutY="11.0" prefHeight="583.0" prefWidth="736.0" spacing="20">
         <padding>
            <Insets bottom="20" left="20" right="20" top="20" />
         </padding>
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="725.0" spacing="10">
               <children>
                  <Label prefHeight="48.0" prefWidth="661.0" styleClass="title" text="Séance Code a venir cette semaine" />
                  <Region prefHeight="58.0" prefWidth="13.0" HBox.hgrow="ALWAYS" />
